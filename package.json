{
  "private": true,
  "name": "spellbook-master",
  "title": "Spellbook Master",
  "description": "This module simplifies the process of creating spellbooks from actors that have spells. Now, creating a spellbook, is as simple as pressing a button and a new item, that has all the spells of the owner actor, will appear in the items tab. ",
  "version": "1.0.7",
  "scripts": {
    "package": "gulp package",
    "build": "gulp clean && gulp build && gulp link",
    "build:watch": "gulp watch",
    "clean": "gulp clean && gulp link --clean",
    "update": "npm install --save-dev @league-of-foundry-developers/foundry-vtt-types@fvtt-0.8.9",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lintfix": "eslint --fix --ext .ts,.js,.vue ./",
    "prettier-format": "prettier --config .prettierrc --write \"./**/*.(ts|js|json|scss|css)\""
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@league-of-foundry-developers/foundry-vtt-types": "^9.255.3",
    "@types/jquery": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "archiver": "^5.2.0",
    "chalk": "^4.1.2",
    "del": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-jquery": "^3.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fs-extra": "^9.1.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.3",
    "gulp-eslint": "^6.0.0",
    "gulp-git": "^2.10.1",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^4.1.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "json-stringify-pretty-compact": "^2.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "yargs": "^15.4.1"
  },
  "lint-staged": {
    "*.(ts|js)": "eslint --fix",
    "*.(json|scss)": "prettier --write"
